
import time
import io

# create a BufferedWriter object to write data to the buffer
buffer = io.BufferedWriter(RawIOBase)

# define the number of seconds for the countdown
countdown_seconds = 5

# use a for loop to print the countdown
for i in range(countdown_seconds, 0, -1):
    buffer.write(i)
    time.sleep(1)

# print the final message
print(" Blast off!")

# flush the data from the buffer to a file
with open("output.txt", "w") as output_file:
    buffer.flush(output_file)
